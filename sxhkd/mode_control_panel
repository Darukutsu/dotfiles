# return to original sxhkd
Escape
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh close;\
        printf "" >"$SXHKD_MODE_FILE"; \
        killall zscroll 2>/dev/null; \
        killall sxhkd && sxhkd &
n
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh next
j
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh next
e
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh prev
k
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh prev
d       
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh rm
x       
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh rm
c       
        "$XDG_CONFIG_HOME"/sxhkd/history_notify.sh clear
